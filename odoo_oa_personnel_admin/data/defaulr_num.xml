<odoo>
    <data noupdate="1">

        <record id="oa_transfer_application_group_number" model="ir.sequence">
            <field name="name">转正申请编号规则</field>
            <field name="code">oa.transfer.application.code</field>
            <field name="prefix">ZA%(year)s%(month)s%(day)s</field>
            <field name="suffix"/>
            <field name="padding">5</field>
        </record>

        <record id="oa_resignation_application_group_number" model="ir.sequence">
            <field name="name">离职申请编号规则</field>
            <field name="code">LZ.resignation.application.code</field>
            <field name="prefix">No%(year)s%(month)s</field>
            <field name="suffix"/>
            <field name="padding">5</field>
        </record>

        <record id="oa_vehicle_application_group_number" model="ir.sequence">
            <field name="name">用车申请编号规则</field>
            <field name="code">oa.vehicle.application.code</field>
            <field name="prefix">YC%(year)s%(month)s</field>
            <field name="suffix"/>
            <field name="padding">5</field>
        </record>

        <record id="oa_itemuse_application_group_number" model="ir.sequence">
            <field name="name">物品领用编号规则</field>
            <field name="code">oa.itemuse.application.code</field>
            <field name="prefix">WP%(year)s%(month)s</field>
            <field name="suffix"/>
            <field name="padding">5</field>
        </record>

        <record id="oa_seal_application_group_number" model="ir.sequence">
            <field name="name">用印申请编号规则</field>
            <field name="code">oa.seal.application.code</field>
            <field name="prefix">YY%(year)s%(month)s</field>
            <field name="suffix"/>
            <field name="padding">5</field>
        </record>

        <record id="oa_general_application_group_number" model="ir.sequence">
            <field name="name">通用审批编号规则</field>
            <field name="code">oa.general.application.code</field>
            <field name="prefix">GR%(year)s%(month)s</field>
            <field name="suffix"/>
            <field name="padding">5</field>
        </record>
        <record id="oa_transfer_application_group_number" model="ir.sequence">
            <field name="name">调岗申请编号规则</field>
            <field name="code">oa.transfer.application.code</field>
            <field name="prefix">DG%(year)s%(month)s</field>
            <field name="suffix"/>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>